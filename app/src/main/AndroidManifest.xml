<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tvvister.contentproviderapplication">
    <uses-permission android:name="android.permission.INTERNET" />
    <permission android:name="tvvister.contentproviderapplication.component.READ"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <provider
            android:name=".component.SingerInfoProvider"
            android:authorities="tvvister.contentproviderapplication.component"
            android:exported="true"
            android:readPermission="tvvister.contentproviderapplication.component.READ"

            />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>